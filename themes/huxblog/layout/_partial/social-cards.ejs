<meta name="author" content="<%- page.author || config.author %>"/>
<meta name="datePublished" content="<%= page.date %>"/>
<meta name="headline" content="<%= page.description || config.description %>"/>
<meta name="publisher" content="<%= config.title %>"/>
<meta itemprop="name" content="<%= page.title || config.name %>">
<meta itemprop="description" content="<%= page.description || config.description %>">

<!-- Schema.org markup for Google+ -->
<meta itemprop="image" content="http://i.imgur.com/XKXsutE.jpg"/>

<!-- Twitter Card data -->
<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:creator" content="@<%= config.twitter_username %>"/>
<meta name="twitter:description" content="<%= page.description || config.description %>">
<meta name="twitter:image" content="http://i.imgur.com/XKXsutE.jpg"/>
<meta name="twitter:site" content="@<%= config.twitter_username %>"/>
<meta name="twitter:title" content="<%= page.title || config.title %>">

<!-- Open Graph data -->

<meta property="og:description" content="<%= page.description || config.description %>" />
<meta property="og:image" content="http://i.imgur.com/XKXsutE.jpg" />
<meta property="og:site_property" content="<%= config.title %>" />
<meta property="og:title" content="<%= page.title || config.title %>" />
<meta property="og:type" content="article" />
<meta property="og:url" content="<%= config.url %>" />

<% if (page.date) { %>
<meta name="article:published_time" content="<%= page.date %>" />
<meta name="article:modified_time" content="<%= page.date %>" />
<% } %>
<meta name="article:section" content="Blog" />
<meta name="fb:admins" content="Facebook numberic ID" />

